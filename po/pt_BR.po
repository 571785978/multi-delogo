msgid "_Output file:"
msgstr "_Arquivo de destino:"

#: ../src/gui/EncodeWindow.cpp:71
msgid "_Select"
msgstr "_Selecionar"

#: ../src/gui/EncodeWindow.cpp:92
msgid "Select output file"
msgstr "Selecione o arquivo de destino"

#: ../src/gui/EncodeWindow.cpp:93 ../src/gui/EncodeWindow.cpp:331
